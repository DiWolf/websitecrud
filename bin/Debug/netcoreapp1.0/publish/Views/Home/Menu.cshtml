@model IEnumerable<elguero.Entities.Menu>
@{
    ViewData["Title"] = "Menu del Restaurante";
}
<!--Fullscreen Image Start-->
<img id="full-screen-background-image" src="../images/slider/2.jpg" alt="img" />
<!--Fullscreen Image End-->
<!--Container Start-->


<section id="container-fluid">
    <section id="container">
        <div class="text_info">
            <h3>Diferentes platillos ofrecidos en El Güero </h3>
            Marisco El Güero  le ofrece una amplia gama de platillos de pescados y mariscos con un exquisito sabor y gran calidad.
        </div>
        <ul class="portfolio_items isotope-container clearfix portfolio-page-template gallery">
            @foreach(var item in Model )
{
   

            <li class="isotope-item all logos">
                <div class="item-container">
                    <img src="@Html.DisplayFor(modelItem => item.Ruta1)" alt="@Html.DisplayFor(modelItem=>item.TextoImagen)" />
                    <a class="link_to_image" data-gal="prettyPhoto[gallery2]" href="@Html.DisplayFor(modelItem => item.Ruta2)"> </a>
                </div>
            </li>
            }
            
        </ul>
    </section>
</section>
<script>
    jQuery(document).ready(function ($) {
        'use strict';
        $("a[data-gal^='prettyPhoto']").prettyPhoto({ hook: 'data-gal' });
    });
</script>